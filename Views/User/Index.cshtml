@model List<MVCCrud1.Models.TableViewModels.UserTableViewModel>
@{
    ViewBag.Title = "Index";
}

<div class="row">
    <div class="col-lg-12">
        <table class="table table-bordered">
            <tr>
                <th>#</th>
                <th>Email</th>
                <th>Edad</th>
            </tr>
            @foreach (var oElemento in Model)
            {
        <tr>
            <td>@oElemento.Id</td>
            <td>@oElemento.Email</td>
            <td>@oElemento.Edad</td>
        </tr>
            }
        </table>
    </div>
</div>

